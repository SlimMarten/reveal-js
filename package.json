{
	"name": "reveal-js",
	"version": "0.0.0",
	"description": "Javascript Library to reveal DOM Elements when entering or leaving visible part of the document",
	"main": "build/reveal.min.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SlimMarten/reveal.git"
	},
	"scripts": {
		"build:prod": "NODE_ENV=production webpack --config config/webpack.config.js --mode production --build",
		"build:dev": "NODE_ENV=development webpack --config config/webpack.config.js --mode development --build",
		"build:app": "NODE_ENV=production webpack --config config/webpack.config.app.js --mode production",
		"build:lib": "npm-run-all build:prod build:dev",
		"build": "npm-run-all build:lib build:app",
		"app": "NODE_ENV=development webpack-dev-server --config config/webpack.config.app.js --mode development --open",
		"start": "NODE_ENV=development webpack-dev-server --config config/webpack.config.js --hot --mode development --open"
	},
	"keywords": ["javascript", "framework", "css", "js", "library", "toolkit"],
	"author": "Marten Zander",
	"license": "MIT",
